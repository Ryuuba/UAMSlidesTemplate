{
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#ff185f",
        "activityBar.background": "#ff185f",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#1d6000",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#ff185f",
        "statusBar.background": "#e40046",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#ff185f",
        "statusBarItem.remoteBackground": "#e40046",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#e40046",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#e4004699",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "latex-workshop.hover.preview.mathjax.extensions": [
        "mathtools"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "cSpell.language": "en,es-ES",
    "peacock.color": "#E40046"
}